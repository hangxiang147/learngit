.getpassbox{width:660px;height:auto;border:1px solid #ddd;position:absolute;z-index:9;padding:20px 10px 70px 10px;}
.getpassbox>h4{font-size:20px;font-weight:normal;padding:0px 10px;color:#3498DB;}
.getpass_flow{width:560px;height:70px;margin-top:40px;}
.getpass_flow ul li{width:33.33%;height:auto;float:left;text-align:center;position:relative;}
.getpass_flow ul li:before,.getpass_flow ul li:after{content: "";position:absolute;width:calc(50% - 20px);border-top:1px solid #428bca;top:20px;display:inline-block;height:0px;}
.getpass_flow ul li:after{left:0px;}
.getpass_flow ul li:before{right:0px;}
.getpass_flow ul li.act{color:#428bca;}
.getpass_flow ul li span{width:40px;height:40px;line-height:40px;background:url(../images/circular_no.png) no-repeat;display:inline-block;color:#666;}
.getpass_flow ul li.act span{background:url(../images/circular.png) no-repeat;color:#fff;}
.getpass_flow ul li p{height:30px;line-height:30px;}
.getpass{width:360px;margin-top:40px;}
.getpass p{position:relative;height:auto;padding:0px 0px 0px 72px;margin:20px 0px;}
.getpass p label{width:72px;height:32px;line-height:32px;position:absolute;left:0px;top:0px;text-align:center;}
.getpass p.user input,.getpass p.yzm input[type=text]{height:32px;line-height:32px;padding:0px 10px;border-radius:2px;-webkit-border-radius:2px;-moz-border-radius:2px;color:#666666;border:1px solid #cccccc;padding:0px 4px;}
.getpass p.user input{width:260px;}
.getpass p.yzm input[type=text]{width:140px;}
.getpass p.yzm input[type=button]{height:34px;line-height:34px;background:#d3003c;display:inline-block;font-size:14px;color:#ffffff;text-align:center;border:0px;-webkit-appearance:none;white-space:nowrap;border-radius:3px;-webkit-border-radius:3px;-moz-border-radius:3px;padding:0px 10px;position:absolute;left:226px;top:0px;}
.getpass p.yzm .yzmbtn{}
.getpass .getpass_btn{height:34px;line-height:34px;background:#3498DB;display:inline-block;font-size:14px;color:#ffffff;text-align:center;border:0px;-webkit-appearance:none;white-space:nowrap;border-radius:3px;-webkit-border-radius:3px;-moz-border-radius:3px;padding:0px 50px;}
.getpass_success{text-align:center;margin-top:40px;}
.getpass_success .success{color:#428bca;font-size:20px;}


.skills{height:auto;position:relative;padding-left:180px;border:1px solid #ddd;width:660px;}
.skills_nav{background:#e7e7e7;width:180px;height:100%;position:absolute;left:0px;top:0px;z-index:1;}
.skills_nav ul li{text-indent:16px;line-height:40px;position:relative;cursor:pointer;}
.skills_nav ul li.act{background:#fff;}
.skills_nav ul li.act:after{content:"";position:absolute;height:24px;width:4px;background:#ff9f20;left:0px;top:8px;}
.skills_box{height:auto;padding:0px 30px;}
.skills_box>ul>li{width:33.33%;height:40px;line-height:40px;float:left;cursor:pointer;}
.skills_box>ul>li>span{position:relative;}
.skills_box>ul>li>span>em{position:absolute;bottom:-27px;left:50%;margin-left:-10px;}
.skills_box>ul>li.skills_box_nav{width:100%;height:auto;line-height:24px;border:1px solid #eeeeee;padding:10px 20px;box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;font-size:12px;cursor:default;}
.skills_box>ul>li.skills_box_nav a{margin-right:20px;height:24px;line-height:24px;display:inline-block;color:#999;}
