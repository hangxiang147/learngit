/*border*/
.bd_gray{border:1px solid #ddd;}
.bdt_gray{border-top:1px solid #ddd;}
.bdr_gray{border-right:1px solid #ddd;}
.bdb_gray{border-bottom:1px dashed #ddd;}
.bdl_gray{border-left:1px solid #ddd;}

.bd_red{border:1px solid #E74C3C;}
.bdt_red{border-top:1px solid #E74C3C;}
.bdr_red{border-right:1px solid #E74C3C;}
.bdb_red{border-bottom:1px dashed #E74C3C;}
.bdl_red{border-left:1px solid #E74C3C;}

.bd_blue{border:1px solid #3498DB;}
.bdt_blue{border-top:1px solid #3498DB;}
.bdr_blue{border-right:1px solid #3498DB;}
.bdb_blue{border-bottom:1px dashed #3498DB;}
.bdl_blue{border-left:1px solid #3498DB;}

.bd_green{border:1px solid #2ECC71;}
.bdt_green{border-top:1px solid #2ECC71;}
.bdr_green{border-right:1px solid #2ECC71;}
.bdb_green{border-bottom:1px dashed #2ECC71;}
.bdl_green{border-left:1px solid #2ECC71;}

.bd_yellow{border:1px solid #f39c12;}
.bdt_yellow{border-top:1px solid #f39c12;}
.bdr_yellow{border-right:1px solid #f39c12;}
.bdb_yellow{border-bottom:1px dashed #f39c12;}
.bdl_yellow{border-left:1px solid #f39c12;}

.bdrs3{-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px;}
.bdrs5{-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;}

.box_sz{box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;}
.box_sd{-moz-box-shadow:0px 0px 2px 2px #888888;box-shadow:0px 0px 2px 2px #888888;}
.text_sd{-moz-text-shadow:0px 0px 2px 2px #888888;text-shadow:0px 0px 2px 2px #888888;}
/*background*/
.bg_999{background:#999;}
.bg_gray{background:#fafafa;}
.bg_red{background:#C0392B;}
