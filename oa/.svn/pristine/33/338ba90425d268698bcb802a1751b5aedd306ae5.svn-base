.bg-gray{background:#fafafa;}
.bg-f7{background:#f7f7f7;}
.bg-f1{background:#F1F1F1;}
.bg-w{background:#ffffff;}

.oa_btn{display:inline-block;font-size:14px;color:#ffffff;text-align:center;border:0px;-webkit-appearance:none;white-space:nowrap;border-radius:3px;-webkit-border-radius:3px;-moz-border-radius:3px;}
.oa_btn:visited,.oa_btn:hover{color:#ffffff;}
.btn_s{line-height:28px;}
.btn_b{line-height:38px;font-size:16px;}

.input,.select,.textarea{border-radius:2px;-webkit-border-radius:2px;-moz-border-radius:2px;color:#666666;}
.input:hover,.select:hover,.textarea:hover{border:1px solid #4693ec;}
.input.error,.select.error,.textarea.error{border:1px solid #ea5b5d;color:#ea5b5d;}
.input{height:26px;line-height:26px;border:1px solid #cccccc;padding:0px 4px;}
.select{border:1px solid #cccccc;height:28px;line-height:28px;padding:0px 15px 0px 2px;}
.textarea{border:1px solid #cccccc;padding:5px;}

.por{position:relative;}
.poa{position:absolute;}
