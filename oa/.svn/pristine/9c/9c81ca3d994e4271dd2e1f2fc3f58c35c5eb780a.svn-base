  <process id="bankAccount" name="银行账户申请" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="applyForBankAccount" name="提出申请"></userTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <userTask id="bankAccount_supervisor_audit" name="主管审批" activiti:assignee="${supervisor}"></userTask>
    <userTask id="bankAccount_manager_audit" name="分管领导审批" activiti:assignee="${manager}"></userTask>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="applyForBankAccount"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="applyForBankAccount" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow3" name="有主管审批" sourceRef="exclusivegateway1" targetRef="bankAccount_supervisor_audit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${supervisor != null}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow4" sourceRef="bankAccount_supervisor_audit" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow5" name="无主管审批" sourceRef="exclusivegateway1" targetRef="bankAccount_manager_audit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${supervisor == null}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" name="通过" sourceRef="exclusivegateway2" targetRef="bankAccount_manager_audit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${supervisorAuditResult == 1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow7" name="不通过" sourceRef="exclusivegateway2" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${supervisorAuditResult == 2}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow9" sourceRef="bankAccount_manager_audit" targetRef="exclusivegateway3"></sequenceFlow>
    <userTask id="bankAccount_finalManager_audit" name="总经理审批" activiti:assignee="${finalManager}"></userTask>
    <sequenceFlow id="flow10" name="不通过" sourceRef="exclusivegateway3" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${managerAuditResult == 2}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" name="通过" sourceRef="exclusivegateway3" targetRef="bankAccount_finalManager_audit">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${managerAuditResult == 1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="bankAccount_finalManager_audit" targetRef="endevent1"></sequenceFlow>
  </process>
